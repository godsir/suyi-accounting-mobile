{
  "name": "suyi-accounting-mobile",
  "version": "1.0.0",
  "description": "Suyi记账移动端Web应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve": "npm run dev",
    "cap:build": "npm run build && npx cap sync",
